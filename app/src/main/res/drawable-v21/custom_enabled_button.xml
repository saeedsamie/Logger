<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:state_pressed="true">
        <shape>
            <solid
                android:color="#acb000">
            </solid>
            <stroke
                android:width="1dp"
                android:color="#acb000">
            </stroke>
        </shape>
    </item>
    <item
        android:state_pressed="false">
        <shape>
            <solid
                android:color="#00b0ac">
            </solid>
            <stroke
                android:width="1dp"
                android:color="#00b0ac">
            </stroke>
        </shape>
    </item>
</selector>